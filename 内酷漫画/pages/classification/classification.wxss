.search{
    display: flex;
    justify-content: space-around;
    align-items: center;
     height: 48px;
    background: gold;
    box-sizing: border-box;
    border-top:1px solid #fff; 
    position: fixed;
    top: 0;
    width: 100%;
}
.search input{
    box-sizing: border-box;
    background: #fff;
    height: 35px;
    line-height: 35px;
    width: 60%;
    text-indent: 1em;
    border-radius: 5px;
}
.search button{
    box-sizing: border-box;
    width: 20%;
    height: 36px;
    font-size: 12px;
    margin: 0;
}
.classfen{
    display: flex;
    justify-content: space-around;
    flex-wrap: wrap;
    padding: 10px;
    margin-top: 48px;
}
.classfen navigator{
    box-sizing: border-box;
    width: 30%;
    /*height: 200px;*/
    text-align: center;
}
.classfen navigator li{
    width: 100%;
}
.classfen navigator li image{
    width: 100%;
    height: 100px;
}